@charset "utf-8";
html,body{color:#333;font:14px/1.6 'Microsoft YaHei','宋体';width:100%;padding:0;margin:0 auto;}
section{ width:100%; margin:0 auto; display:none;}
.clear{ clear:both;}
header{
	width:100%;
	height:75px;
	padding:30px 0px 15px 0px;
	margin-bottom:10px;
	text-align:center;
	background:url('../images/header.png') no-repeat center center;		
}
header img{
	display:inline-block;
	max-width:800px;	
}
footer{
	text-align:center;
	padding:10px 0;
}
#main{
	width:1200px;
	margin:0 auto;
}
#left{
	float:left;
	width:200px;
	padding:10px 0;
	background:url('../images/sp.png') no-repeat right center;
}
#left span{
	display:inline-block;
	width:100%;
	height:80px;
	margin:5px auto;
	cursor:pointer;
}
#left span.menu1{
	background:url('../images/left1.png') no-repeat center center;
}
#left span.menu1_on{
	background:url('../images/left1-2.png') no-repeat center center;
}
#left span.menu2{
	background:url('../images/left2.png') no-repeat center center;
}
#left span.menu2_on{
	background:url('../images/left2-2.png') no-repeat center center;
}
#left span.menu3{
	background:url('../images/left3.png') no-repeat center center;
}
#left span.menu3_on{
	background:url('../images/left3-2.png') no-repeat center center;
}
#left span.menu4{
	background:url('../images/left4.png') no-repeat center center;
}
#left span.menu4_on{
	background:url('../images/left4-2.png') no-repeat center center;
}
#left span.menu5{
	background:url('../images/left5.png') no-repeat center center;
}
#left span.menu5_on{
	background:url('../images/left5-2.png') no-repeat center center;
}
#right{
	float:right;
	width:1000px;	
}
#right section{
	margin-left:60px;
	width:850px;
}
#right section h2{

}
#right section div.one_item{
	color:#999;
	margin-bottom:80px;
}
#right section div.one_item select{
	width:350px;
	height:28px;
	color:#999;
	display:block;
	margin-bottom:15px;
}
.button{
	background:url("../images/off.png") no-repeat center center;
	display:inline-block;
	width:120px;
	height:26px;
	border:none;
	cursor:pointer;
	border-radius:3px;
	color:#fff;
	font-weight:bold;
	font-size:14px;
}
.second{
	margin-left:106px;
}
textarea{width:99%;height:80px;margin:5px auto;}
.on{background:url("../images/on.png") no-repeat center center;}
.fr{float:right;}
table tr td{
	padding:5px;
	color:#666;
	
}

table tr td select{
	padding:0px 5px;

}
.orange{
	background:url('../images/orange.png') no-repeat center center;
}
.mright50{margin-right:50px;}
.nav{ 
	background:url('../images/port1.png') no-repeat center center;
	height:56px;
}
.nav2{
	background:url('../images/port2.png') no-repeat center center;	
	height:56px;
}

.nav span{
	cursor:pointer;
	display:inline-block;
	height:27px;
	line-height:27px;
	font-size:16px;
	width:100px;
	text-align:center;
	font-weight:bold;
	color:#666;

}
.nav span:first-child{
	cursor:pointer;
	display:inline-block;
	height:27px;
	line-height:27px;
	font-size:16px;
	width:135px;
	text-align:center;
	font-weight:bold;
	color:#666;

}
.nav2 span{
	cursor:pointer;
	display:inline-block;
	height:27px;
	line-height:27px;
	font-size:16px;
	width:100px;
	text-align:center;
	font-weight:bold;
	color:#666;

}
.nav2 span:first-child{
	cursor:pointer;
	display:inline-block;
	height:27px;
	line-height:27px;
	font-size:16px;
	width:120px;
	text-align:center;
	font-weight:bold;
	color:#666;

}
.nav_item{
	background-color:rgb(244,248,251);
	border-radius:0px 0px 15px 15px; 
	border:1px solid rgb(225,225,225);
	border-top:0px;
	padding:0px 50px 30px 50px;
	width:748px;
	margin-top:-1px;
}
input.site{
	width:99%;
}